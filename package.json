{
  "name": "cautious-guide",
  "version": "1.0.0",
  "description": "Node.js Service",
  "main": "index.js",
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.5.0",
    "yarn": ">=1.5.1"
  },
  "scripts": {
    "start": "gulp serve",
    "start:debug": "cross-env DEBUG=cautious-guide:* npm start",
    "build": "gulp",
    "lint": "esw *.js src config --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register test --recursive",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test istanbul cover _mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register test --recursive",
    "test:check-coverage": "npm run test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "dependencies": {
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cors": "2.8.1",
    "debug": "4.1.0",
    "dotenv": "4.0.0",
    "express": "4.14.0",
    "express-validation": "1.0.1",
    "express-winston": "2.1.2",
    "helmet": "3.1.0",
    "http-status": "0.2.0",
    "joi": "10.0.6",
    "lodash": "4.17.11",
    "mongoose": "5.0.15",
    "morgan": "1.7.0",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-stage-2": "6.18.0",
    "chai": "3.4.1",
    "commitizen": "2.9.2",
    "coveralls": "2.11.6",
    "cross-env": "3.1.3",
    "cz-conventional-changelog": "1.2.0",
    "del": "2.2.0",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-load-plugins": "1.2.0",
    "gulp-newer": "1.1.0",
    "gulp-nodemon": "2.0.6",
    "gulp-sourcemaps": "1.6.0",
    "gulp-util": "3.0.8",
    "husky": "0.13.1",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.2.0",
    "run-sequence": "1.1.5",
    "supertest": "2.0.1",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "2.6.1"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
